{
    "name": "shaonmajumder/log-viewer",
    "description": "Simple multi-directory log-viewer for laravel.",
    "keywords": [
        "laravel",
        "log",
        "viewer",
        "log viewer",
        "multi-directory",
        "log management",
        "log monitoring",
        "log files",
        "log analysis",
        "laravel package",
        "laravel log viewer",
        "laravel log management",
        "laravel log monitoring",
        "log file viewer",
        "log file management",
        "log file monitoring",
        "laravel development",
        "laravel tool",
        "laravel utility"
    ],
    "homepage": "https://shaonmajumder.github.io/log-viewer/",
    "license": "MIT",
    "authors": [
        {
            "name": "Shaon Majumder",
            "email": "smazoomder@gmail.com"
        }
    ],
    "version": "0.0.0.1",
    
    "require": {
        "illuminate/support": "^8.0|^9.0|^10.0",
        "illuminate/config": "^8.0|^9.0|^10.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*|^9.6.3"
    },
    "autoload": {
        "psr-4": {
            "Shaon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shaon\\ServiceProvider\\LogViewerServiceProvider"
            ],
            "aliases": {
                "MapBox": "Shaon\\Facades\\LogView"
            }
        }
    },
    "minimum-stability": "stable", 
    "prefer-stable": true 
}